{
  "name": "tech_news_scraper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "packageManager": "yarn@3.6.0",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node --max-old-space-size=700 -r ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./src/app.ts",
    "build": "NODE_ENV=production tsc --project tsconfig.json --diagnostics  -p tsconfig.json",
    "dev": "NODE_ENV=development node --max-old-space-size=700 -r ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./src/app.ts",
    "dev:ml": "NODE_ENV=development node --max-old-space-size=700 -r ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./src/libraries/scrape/machine.lib.ts",
    "schema": "prisma generate",
    "migrate": "prisma migrate dev --name added_melon",
    "db:update": "prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "json2csv": "^6.0.0-alpha.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.1",
    "prisma": "^5.0.0",
    "utf8": "^3.0.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^20.4.2",
    "@types/node-cron": "^3.0.8",
    "@types/utf8": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
